      <section class="notification"></section>
          <script>
          $("#menu-toggle").click(function(e) {
                  e.preventDefault();
                  $("#wrapper").toggleClass("toggled");
                   });
            Pusher.log = function(message) {
              if (window.console && window.console.log) {
                window.console.log(message);
              }
            };
            toastr.options.closeButton = true;
            var pusher = new Pusher('90b3bfd4d3713a081765');
            var channel = pusher.subscribe('test_channel'+<%=current_user.id.to_s%>);
            channel.bind('new_notification', function(notification){
                  var message = notification.message;
                  toastr.error(message)
              });

            channel.bind('alarm', function(notification){
                  var message = notification.message;
                  var titulo = notification.titulo;
                  toastr["info"](message,titulo)
              });

            channel.bind('heart', function(notification){
                  var message = notification.message;
                  var titulo = notification.titulo;
                  toastr["warning"](message,titulo)
              });
          </script>

    
